{
    "name": "h3script",
    "displayName": "H3script",
    "publisher": "Crisp",
    "author": {
		"name": "Crisp"
	},
    "description": "Provides syntax highlighting support for Halo 3 Scripting",
    "keywords": [ "hsc", "haloscript", "language", "blamscript", "halo3script", "h3script"],
    "galleryBanner": {
		"color": "#007acc",
		"theme": "dark"
	},
    "preview": true,
    "version": "1.0.0",
    "license": "MIT",
    "icon": "h3script_icon_small.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ILoveAGoodCrisp/H3Script"
    },
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages",
        "Language Packs"
    ],
    "contributes": {
        "languages": [{
            "id": "hsc",
            "aliases": ["h3script", "hsc"],
            "extensions": [".hsc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "hsc",
            "scopeName": "source.hsc",
            "path": "./syntaxes/hsc.tmLanguage.json"
        }]
    }
}