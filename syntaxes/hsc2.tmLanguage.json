{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Halo 2 Script",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#expression"
		}
	],
	"repository": {
		"comment":{
			"patterns": [
				{
					"name": "comment.block.hsc2",
					"begin": ";\\*",
					"end": "\\*;",
					"captures":{
						"0":{
							"name": "comment.block.hsc2"
						}
					}
				},
				{
					"name": "comment.line",
					"begin": ";[^*]",
					"end": "$",
					"captures":{
						"0":{
							"name": "comment.line.hsc2"
						}
					}
				}
			]
		},
		"expression": {
			"begin": "\\(",
			"end": "\\)",
			"beginCaptures": { 
				"0": { "name": "punctuation.paren.open.hsc2" }
			},
			"endCaptures": { 
				"0": { "name": "punctuation.paren.close.hsc2" }
			},
			"patterns": [
				{
					"include": "#comment"
				},
				{
					"include": "#expression"
				},
				{
					"include": "#strings"
				},
				{
					"include": "#declarations"
				},
				{
					"include": "#scripts"
				},
				{
					"include": "#types"
				},
				{
					"include": "#controls"
				},
				{
					"include": "#operators"
				},
				{
					"include": "#numbers"
				},
				{
					"include": "#enums"
				},
				{
					"include": "#globals"
				},
				{
					"include": "#functions"
				},
				{
					"include": "#variables"
				}
			]
		},
		"strings":{
			"patterns": [
				{
					"name": "string.quoted.double.hsc2",
					"begin": "\"",
					"end": "\""

				}
			]
		},
		"declarations":{
			"patterns": [
				{
					"name": "entity.name.class.hsc2",
					"match":"(?<=^\\s*\\(\\s*)(script|global)"
				}
			]
		},
		"scripts":{
			"patterns": [
				{
					"name":"entity.name.tag.hsc2",
					"match": "(?<=script\\s*)\\b(continuous|command_script|dormant|startup|static|stub)\\b"
				}
			]
		},
		"types":{
			"patterns": [
				{
					"name":"storage.type.hsc2",
					"match": "(?<=\\(|static\\s|stub\\s|global\\s)\\b(void|boolean|real|short|long|string|string_id|ai|object|unit|vehicle|weapon|device|scenery|object_list|object_name|unit_name|vehicle_name|weapon_name|device_name|scenery_name|sound|effect|damage|looping_sound|animation_graph|damage_effect|object_definition|script|unit_seat_mapping|trigger_volume|cutscene_flag|cutscene_camera_point|cutscene_title|cutscene_recording|device_group|point_reference|ai_command_list|ai_command_script|ai_behavior|ai_orders|starting_profile|conversation|navpoint|team|hud_message|structure_bsp|style|bitmap|shader|render_model|structure_definition|lightmap_definition|game_difficulty|hud_corner|actor_type|model_state|network_event|function_name|passthrough)\\b"
				}
			]
		},
		"operators":{
			"patterns": [
				{
					"name":"keyword.operator.hsc2",
					"match": "(\\!=|\\*|\\+|\\-|/|%|\\<|\\<=|=|\\>|\\>=|=|\\b(and|or|not)\\b)"
				}
			]
		},
		"numbers":{
			"patterns": [
				{
					"name":"constant.numeric.hsc2",
					"match": "(?i)\\b(?:true|none|false|\\d+(?:\\.\\d+)?)\\b|\\b(default|player|human|covenant|flood|sentinel|heretic|prophet|guilty|unused9|unused10|unused11|unused12|unused13|unused14|unused15|easy|normal|heroic|legendary|top_left|top_right|bottom_left|bottom_right|center|elite|jackal|grunt|hunter|engineer|assassin|player|marine|crew|combat_form|infection_form|carrier_form|monitor|sentinel|none|mounted_weapon|brute|prophet|bugger|juggernaut|standard|minor_damage|medium_damage|major_damage|destroyed|verbose|status|message|warning|error|critical)\\b"
				}
			]
		},
