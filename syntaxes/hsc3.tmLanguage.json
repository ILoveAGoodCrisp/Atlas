{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Halo 3 Script",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#expression"
		}
	],
	"repository": {
		"comment":{
			"patterns": [
				{
					"name": "comment.block.hsc3",
					"begin": ";\\*",
					"end": "\\*;",
					"captures":{
						"0":{
							"name": "comment.block.hsc3"
						}
					}
				},
				{
					"name": "comment.line",
					"begin": ";[^*]",
					"end": "$",
					"captures":{
						"0":{
							"name": "comment.line.hsc3"
						}
					}
				}
			]
		},
		"expression": {
			"begin": "\\(",
			"end": "\\)",
			"beginCaptures": { 
				"0": { "name": "punctuation.paren.open.hsc3" }
			},
			"endCaptures": { 
				"0": { "name": "punctuation.paren.close.hsc3" }
			},
			"patterns": [
				{
					"include": "#comment"
				},
				{
					"include": "#expression"
				},
				{
					"include": "#strings"
				},
				{
					"include": "#declarations"
				},
				{
					"include": "#scripts"
				},
				{
					"include": "#types"
				},
				{
					"include": "#controls"
				},
				{
					"include": "#operators"
				},
				{
					"include": "#numbers"
				},
				{
					"include": "#enums"
				},
				{
					"include": "#globals"
				},
				{
					"include": "#functions"
				},
				{
					"include": "#variables"
				}
			]
		},
		"strings":{
			"patterns": [
				{
					"name": "string.quoted.double.hsc3",
					"begin": "\"",
					"end": "\""

				}
			]
		},
		"declarations":{
			"patterns": [
				{
					"name": "entity.name.class.hsc3",
					"match":"(?<=^\\s*\\(\\s*)(script|global)"

				}
			]
		},
		"scripts":{
			"patterns": [
				{
					"name":"entity.name.tag.hsc3",
					"match": "(?<=script\\s*)\\b(continuous|command_script|dormant|startup|static|stub)\\b"
				}
			]
		},
		"types":{
			"patterns": [
				{
					"name":"storage.type.hsc3",
					"match": "(?<=\\(|static\\s|stub\\s|global\\s)\\b(void|boolean|real|short|long|string|string_id|ai|object|unit|vehicle|weapon|device|scenery|effect_scenery|object_list|object_name|unit_name|vehicle_name|weapon_name|device_name|scenery_name|sound|effect|damage|looping_sound|animation_graph|damage_effect|object_definition|script|unit_seat_mapping|trigger_volume|cutscene_flag|cutscene_camera_point|cutscene_title|cutscene_recording|device_group|point_reference|ai_command_list|ai_command_script|ai_behavior|ai_orders|ai_line|starting_profile|conversation|team|mp_team|zone_set|designer_zone|style|folder|bitmap|shader|render_model|structure_definition|lightmap_definition|cinematic_definition|cinematic_scene_definition|bink_definition|any_tag|any_tag_not_resolving|game_difficulty|controller|button_preset|joystick_preset|player_color|player_character_type|voice_output_setting|voice_mask|subtitle_setting|actor_type|model_state|event|character_physics|effect_scenery_name|cinematic_lightprobe|animation_budget_reference|looping_sound_budget_reference|sound_budget_reference|function_name|passthrough)\\b"
				}
			]
		},
		"operators":{
			"patterns": [
				{
					"name":"keyword.operator.hsc3",
					"match": "(\\!=|\\*|\\+|\\-|/|%|\\<|\\<=|=|\\>|\\>=|=|\\b(and|or|not)\\b)"
				}
			]
		},
		"numbers":{
			"patterns": [
				{
					"name":"constant.numeric.hsc3",
					"match": "(?i)\\b(?:true|none|false|\\d+(?:\\.\\d+)?)\\b|\\b(default|player|human|covenant|flood|sentinel|heretic|prophet|guilty|unused9|unused10|unused11|unused12|unused13|unused14|unused15|mp_team_red|mp_team_blue|mp_team_green|mp_team_yellow|mp_team_purple|mp_team_orange|mp_team_brown|mp_team_grey|easy|normal|heroic|legendary|controller1|controller2|controller3|controller4|standard|community|boxer|green_thumb|professional|evolved|community2|universal_recon|universal_reclaimer|universal_zoom_and_shoot|universal_bumper_jumper|universal_green_fingers|standard|south_paw|legacy|legacy_south_paw|player_color_00|player_color_01|player_color_02|player_color_03|player_color_04|player_color_05|player_color_06|player_color_07|player_color_08|player_color_09|player_color_10|player_color_11|player_color_12|player_color_13|player_color_14|player_color_15|player_color_16|player_color_17|player_color_18|player_color_19|player_color_20|player_color_21|player_color_22|player_color_23|player_color_24|player_color_25|player_color_26|player_color_27|player_color_28|player_color_29|player_color_30|player_color_31|sp_masterchief|sp_dervish|mp_masterchief|mp_elite|coop_elite|monitor|default|headset|none|anonymous|automatic|enabled|disabled|elite|jackal|grunt|hunter|engineer|assassin|player|marine|crew|combat_form|infection_form|carrier_form|monitor|sentinel|none|mounted_weapon|brute|prophet|bugger|juggernaut|pure_form_stealth|pure_form_tank|pure_form_ranged|scarab|guardian|standard|minor_damage|medium_damage|major_damage|destroyed|verbose|status|message|warning|error|critical|none|none|player|biped)\\b"
				}
