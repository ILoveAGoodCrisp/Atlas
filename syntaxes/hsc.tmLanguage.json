{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "h3script",
	"patterns": [
		{
			"include": "#scriptContainer"
		}
	],
	"repository": {
		"scriptContainer":{
			"patterns":[
				{
					"include": "#comment"
				},
				{
					"include": "#expression"
				}
			]
		},
		"expression": {
			"begin": "\\(",
			"end": "\\)",
			"beginCaptures": { 
				"0": { "name": "punctuation.paren.open" }
			},
			"endCaptures": { 
				"0": { "name": "punctuation.paren.close" }
			},
			"patterns": [
				{
					"include": "#scriptContainer"
				},
				{
					"include": "#comment"
				},
				{
					"include": "#expression"
				},
				{
					"include": "#number"
				},
				{
					"include": "#string"
				},
				{
					"include": "#operator"
				},
				{
					"include": "#boolean"
				},
				{
					"include": "#class"
				},
				{
					"include": "#scriptType"
				},
				{
					"include": "#enum"
				},
				{
					"include": "#valueType"
				},
				{
					"include": "#engineGlobal"
				},
				{
					"include": "#scriptFunction"
				},
				{
					"include": "#scriptName"
				}
			]
		},
		"comment":{
			"patterns": [
				{
					"name": "comment.line",
					"begin": ";[^*]",
					"end": "$\\n?",
					"captures":{
						"0":{
							"name": "comment.line"
						}
					}
				},
				{
					"name": "comment.block",
					"begin": ";\\*",
					"end": "\\*;",
					"captures":{
						"0":{
							"name": "comment.block"
						}
					}
				}
			]
		},
		"number":{
			"patterns": [
				{
					"name": "constant.numeric",
					"match": "(?<=\\s)[\\d.-]+\\b"
				}
			]
		},
		"string":{
			"patterns": [
				{
					"name": "string.quoted",
					"begin": "\"",
					"end": "\""
				}
			]
		},
		"operator":{
			"patterns": [
				{
					"name": "keyword.operator",
					"match": "(?<=\\(\\s*)(\\+|-|\\*|/|=|!=|>=|<=|>|<)"
				}
			]
		},
		"boolean":{
			"patterns":[
				{
					"name": "constant.character",
					"match": "(?i)\\b(true|false|on|off)\\b"
				}
			]
		},
		"class":{
			"patterns":[
				{
					"name": "storage.type",
					"match": "(?i)(script|global)(?=\\s+(startup|dormant|continuous|static|command_script|stub|unparsed|special_form|function_name|passthrough|void|boolean|real|short|long|string|script|string_id|unit_seat_mapping|trigger_volume|cutscene_flag|cutscene_camera_point|cutscene_title|cutscene_recording|device_group|ai|ai_command_list|ai_command_script|ai_behavior|ai_orders|ai_line|starting_profile|conversation|zone_set|designer_zone|point_reference|style|object_list|folder|sound|effect|damage|looping_sound|animation_graph|damage_effect|object_definition|bitmap|shader|render_model|structure_definition|lightmap_definition|cinematic_definition|cinematic_scene_definition|bink_definition|any_tag|any_tag_not_resolving|game_difficulty|team|mp_team|controller|button_preset|joystick_preset|player_color|player_character_type|voice_output_setting|voice_mask|subtitle_setting|actor_type|model_state|event|character_physics|object|unit|vehicle|weapon|device|scenery|effect_scenery|object_name|unit_name|vehicle_name|weapon_name|device_name|scenery_name|effect_scenery_name|cinematic_lightprobe|animation_budget_reference|looping_sound_budget_reference|sound_budget_reference))\\b"
				}
			]
